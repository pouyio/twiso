{
  "env": {
    "VITE_TRAKT_API_KEY": "@react_app_trakt_api_key",
    "VITE_TMDB_API_KEY": "@react_app_tmdb_api_key",
    "VITE_REDIRECT_URL": "@react_app_redirect_url",
    "SENTRY_DSN": "@sentry_dsn"
  },
  "functions": {
    "api/server.ts": {
      "runtime": "vercel-deno@3.0.1"
    }
  },
  "rewrites": [
    {
      "source": "/movie/:id",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
