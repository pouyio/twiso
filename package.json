{
  "name": "twiso",
  "version": "1.6.9",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "axios-rate-limit": "^1.2.1",
    "dexie": "^2.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fastclick": "^3.0.2",
    "react-helmet": "^5.2.1",
    "react-hook-inview": "^4.0.0",
    "react-image": "^2.3.0",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.3.0",
    "use-query-params": "^0.4.1"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "tailwind:css": "tailwind build src/tailwind.src.css -c tailwind.js -o src/tailwind.css",
    "start": "npm run tailwind:css && react-scripts start",
    "build": "npm run tailwind:css && postcss ./src/tailwind.css -r --env production && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.14",
    "@types/react-router-dom": "^5.1.0",
    "conventional-changelog-atom": "^2.0.3",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.31",
    "source-map-explorer": "^2.2.1",
    "tailwindcss": "^1.1.4",
    "typescript": "3.7.2"
  }
}
